{
  "mcpServers": {
    "postgres": {
      "args": ["postgres-mcp", "--access-mode=unrestricted"],
      "command": "uvx",
      "disabled": true,
      "env": {
        "DATABASE_URI": "postgresql://${DB_USERNAME}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_DATABASE}"
      }
    },
    "language-server": {
      "command": "mcp-language-server",
      "args": ["--workspace", "${CWD}", "--lsp", "gopls"]
    }
  }
}
